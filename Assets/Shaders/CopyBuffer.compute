#pragma kernel CSCopyAppendtoConsumeBuffer

#include "./Utility.hlsl"

ConsumeStructuredBuffer<TerrainInstanceData> srcConsumeBuffer;
AppendStructuredBuffer<TerrainInstanceData> dstAppendBuffer;

[numthreads(1,1,1)]
void CSCopyAppendtoConsumeBuffer(uint3 id : SV_DispatchThreadID)
{
    TerrainInstanceData node = srcConsumeBuffer.Consume();
    dstAppendBuffer.Append(node);
}